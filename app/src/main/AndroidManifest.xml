<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="stock.register.godown.stock.management.shop.stock.ledger">

    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Bhandar">
        <activity
            android:name="stock.register.godown.stock.management.shop.stock.ledger.activities.AddStoreAndPartyActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoActionBar"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="stock.register.godown.stock.management.shop.stock.ledger.activities.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>